{
  "name": "anki-to-quizlet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "server:refactored": "node server/index-refactored.js",
    "dev:server": "nodemon server/index.js",
    "dev:server:refactored": "nodemon server/index-refactored.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "dev:full:refactored": "concurrently \"npm run dev\" \"npm run dev:server:refactored\"",
    "start": "npm run build && concurrently \"npm run preview\" \"npm run server\"",
    "deploy": "npm run build && gh-pages -d dist",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "@vueuse/core": "^13.6.0",
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jszip": "^3.10.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "papaparse": "^5.5.3",
    "sqlite3": "^5.1.7",
    "vite-plugin-pwa": "^1.0.2",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "concurrently": "^9.2.0",
    "gh-pages": "^6.3.0",
    "nodemon": "^3.1.10",
    "vite": "^7.0.4"
  }
}
